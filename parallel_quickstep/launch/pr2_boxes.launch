<launch>

  <!-- so ros know we're simming -->
  <param name="/use_sim_time" value="true" />

  <!-- start gazebo with an empty plane -->
  <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find parallel_quickstep)/worlds/box_stack_cuda.world" respawn="false" output="screen"/>

  <!-- start pr2 robot -->
  <include file="$(find pr2_gazebo)/pr2.launch"/>

  <param name="robot_description" command="$(find xacro)/xacro.py '$(find parallel_quickstep)/urdf/box_stack_block_interlaced.urdf.xacro'" />

  <node name="spawn_box_stack_block_interlaced_cuda" pkg="gazebo" type="spawn_model" args="-urdf -param robot_description -model box_stack_block_interlaced_cuda" respawn="false" output="screen" />

</launch>

